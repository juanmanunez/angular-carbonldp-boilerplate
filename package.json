{
  "name": "angular-jspm-carbonldp-boilerplate",
  "version": "1.1.0",
  "description": "A boilerplate to get you started creating web applications using Angular + Webpack + CarbonLDP",
  "main": "src/index.html",
  "scripts": {
    "build": "npm run build:prod",
    "build:prod": "npm run clean:dist && node_modules/.bin/webpack --config config/webpack.prod.js --progress --profile --bail",
    "clean:dist": "rimraf dist",
    "start": "npm run server:dev",
    "server:dev": "node_modules/.bin/webpack-dev-server --config config/webpack.dev.js --inline --progress --profile --watch "
  },
  "author": "Miguel Aragón <miguel.aragon@base22.com>",
  "contributors": [
    {
      "name": "Miguel Aragón",
      "email": "miguel.aragon@base22.com"
    },
    {
      "name": "Alvaro Contreras",
      "email": "alvaro.contreras@base22.com"
    }
  ],
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "carbonldp": "~1.0.0-alpha.7",
    "core-js": "^2.5.3",
    "reflect-metadata": "0.1.10",
    "rxjs": "^5.5.6",
    "sparqler": "^0.3.0",
    "zone.js": "^0.8.20"
  },
  "devDependencies": {
    "@angular/compiler-cli": "5.2.0",
    "@ngtools/webpack": "1.9.4",
    "@types/node": "^9.3.0",
    "angular-router-loader": "^0.8.2",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.4.1",
    "copy-webpack-plugin": "^4.3.1",
    "file-loader": "^1.1.6",
    "html-loader": "^0.5.4",
    "html-webpack-plugin": "2.30.1",
    "node-sass": "4.7.2",
    "raw-loader": "^0.5.1",
    "rimraf": "2.6.2",
    "sass-loader": "^6.0.6",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.2",
    "webpack-dev-server": "^2.11.0",
    "webpack-merge": "4.1.1"
  }
}
