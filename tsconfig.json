{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es5",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"noImplicitAny": false,
		"moduleResolution": "node",
		"lib": [
			"es2015",
			"dom"
		],
		"baseUrl": ".",
		"paths": {
			"app/*": [
				"src/app/*"
			]
		}
	},
	"exclude": [
		"dist",
		"node_modules"
	]
}